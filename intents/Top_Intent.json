{
    "name": "Top_Intent", 
    "description": "Top five events by dimension",
    "sampleUtterances": [
        "Top {count} {dimension} by revenue",
        "Top {count} {dimension} based on revenue",
        "Top {count} {dimension} by revenue for {event_date}",
        "Top {count} {dimension} based on revenue for {event_date}"
    ], 
    "slots": [
        {
            "slotType": "AMAZON.NUMBER", 
            "name": "count", 
            "slotConstraint": "Required", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "number", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "sampleUtterances": []
        }, 
        {
            "slotType": "dimensions", 
            "name": "dimension", 
            "slotConstraint": "Required", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "dimension", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 2, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": []
        },
        {
            "slotType": "AMAZON.DATE", 
            "name": "event_date", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The date of the event", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 3, 
            "sampleUtterances": [], 
            "description": "event_date"
        }
    ], 
    "conclusionStatement": {
      "messages": [
        {
          "content": "via code hook",
          "contentType": "PlainText"
        }
      ]
    },
    "fulfillmentActivity": {
        "type": "CodeHook", 
        "codeHook": {
            "uri": "{{lambda-arn}}", 
            "messageVersion": "1.0"
        }
    }
}
